              <a href="notice-detail/<%=글목록[i]._id%>"><%=글목록[i].title%></a>
            </h4>
            <span>한대규</span>
            <span style="padding-left: 15px ;"><%=글목록[i].today%></span>
          </div>
          <%}%>
      
    </div>

